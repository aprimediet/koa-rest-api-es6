{
  "name": "koa-rest-api-es6",
  "version": "0.0.1",
  "description": "A simple Koa 2 rest api that implement oauth2 Resource Owner Password Flow with JSON Web Token and Refresh Token.",
  "author": {
    "name": "Damien Dell'Amico",
    "email": "damien.dellamico@gmail.com",
    "url": "https://github.com/ddellamico"
  },
  "main": "babel.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node babel.js",
    "build": "npm run clean && npm run babel && node ./dist/app",
    "babel": "./node_modules/.bin/babel src -d dist --source-maps",
    "dev": "./node_modules/.bin/nodemon --debug -L babel.js --ext 'js json'",
    "debug": "export DEBUG=krs:* && npm run dev",
    "lint": "./node_modules/.bin/eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "postmerge": "npm install"
  },
  "pre-commit": [
    "lint",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ddellamico/koa-rest-api-es6"
  },
  "engines": {
    "node": ">=5.x",
    "npm": ">=3.x"
  },
  "bugs": {
    "url": "https://github.com/ddellamico/koa-rest-api-es6/issues"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "koa",
    "koa2",
    "koa v2",
    "babel",
    "es6",
    "JWT",
    "jsonwebtoken",
    "oauth2",
    "node",
    "rest",
    "mongoose",
    "mongo",
    "docker",
    "docker-compose",
    "vagrant"
  ],
  "dependencies": {
    "bcrypt": "^0.8.6",
    "bluebird": "3.3.3",
    "boom": "^3.1.2",
    "debug": "*",
    "dotenv": "^2.0.0",
    "jsonwebtoken": "^5.7.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-generic-session": "^1.10.1",
    "koa-helmet": "^2.0.0-alpha.1",
    "koa-jwt": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mount": "github:coderhaoxin/koa-mount#next",
    "koa-passport": "^2.1.1",
    "koa-router": "^7.0.1",
    "mongoose": "^4.4.14",
    "mongoose-validator": "^1.2.4",
    "node-uuid": "^1.4.7",
    "oauth2orize-koa": "^1.3.2",
    "passport-http": "^0.3.0",
    "passport-jwt": "^2.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "source-map-support": "^0.4.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-root-import": "^3.2.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "5.0.1",
    "eslint-plugin-react": "3.16.1",
    "nodemon": "^1.9.2",
    "pre-commit": "^1.1.2"
  },
  "license": "MIT"
}
