{
  "name": "koa-rest-api-es6",
  "version": "0.0.1",
  "description": "Koa & ES6 REST API",
  "author": {
    "name": "Damien Dell'Amico",
    "email": "damien.dellamico@gmail.com",
    "url": "https://github.com/ddellamico"
  },
  "main": "index.js",
  "scripts": {
    "start": "node babel.js",
    "prod": "node ./dist/index",
    "babel": "./node_modules/.bin/babel src -d dist --source-maps",
    "dev": "./node_modules/.bin/nodemon --debug -L babel.js --ext 'js json'",
    "debug": "export DEBUG=krs:* && npm run dev",
    "clean": "rm -rvf dist",
    "lint": "./node_modules/.bin/eslint --cache .",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "postmerge": "npm install"
  },
  "pre-commit": [
    "lint",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ddellamico/koa-rest-api-es6"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=2.x"
  },
  "bugs": {
    "url": "https://github.com/ddellamico/koa-rest-api-es6/issues"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "koa",
    "babel",
    "es6",
    "node"
  ],
  "dependencies": {
    "bcrypt-as-promised": "^1.1.0",
    "bluebird": "2.10.2",
    "debug": "*",
    "dotenv": "^1.2.0",
    "jsonwebtoken": "^5.7.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-generic-session": "^1.10.1",
    "koa-jwt": "^1.1.2",
    "koa-logger": "^2.0.0",
    "koa-passport": "^2.0.1",
    "koa-router": "^7.0.1",
    "mongoose": "^4.4.5",
    "mongoose-validator": "^1.2.4",
    "oauth2orize": "^1.2.2",
    "oauth2orize-koa": "^1.3.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-spread": "^6.5.2",
    "babel-preset-es2015-node4": "^2.0.3",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.5.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "5.0.1",
    "eslint-plugin-react": "3.16.1",
    "nodemon": "^1.9.0"
  },
  "license": {
    "type": "MIT"
  }
}
